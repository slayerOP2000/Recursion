void insert(stack<int> &stack,int temp)
{
	if(stack.size()==0 || stack.top()<=temp)
    {
        stack.push(temp);
        return;
    }
    int value = stack.top();
    stack.pop();
    insert(stack,temp);
    stack.push(value);
    return;
}
void sortStack(stack<int> &stack)
{
	// Write your code here
    if(stack.size() == 1)
    {
        return;
    }
    int temp = stack.top();
    stack.pop();
    sortStack(stack);
    insert(stack,temp);
    return;
}
